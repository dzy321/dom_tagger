{
  "extends": "airbnb-base",
  "rules": {
    "comma-dangle": 0,
    "func-names": 0,
    "semi": ["error", "never"],
    "no-param-reassign": "off",
    "global-require": "off",
    "arrow-body-style": "off",
    "no-continue": 0,
    "no-plusplus": 0,
    "no-undef": 0,
    "one-var-declaration-per-line": 0,
    "one-var": 0,
    "quotes": ["error", "single", {"allowTemplateLiterals": true}]
  }
}
